  "license": "ISC",
  "bugs": {
    "url": "https://github.com/sivivan/miamiboat/issues"
  },
  "homepage": "https://github.com/sivivan/miamiboat#readme",
  "dependencies": {
    "express": "^4.16.4",
    "forever": "^0.15.3",
    "http-server": "^0.11.1",
    "nodemon": "^1.18.10"
  },
  "devDependencies": {
    "npm-watch": "^0.6.0"
  }
}
